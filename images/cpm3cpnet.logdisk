	lxi	d,msgfcb
	lxi	b,FCBLEN
	ldir
	lxi	h,tmpsuf
	lxi	d,mailfcb+FCBTYP
	lxi	b,3
	ldir
	lxi	d,dskbuf
	mvi	c,SETDMA
	call	BDOS
	lxi	d,msgfcb
	mvi	c,OPEN
	call	BDOS
	cpi	0ffh
	jz	op2error
	lxi	d,mailfcb
	mvi	c,MAKE
	call	BDOS
	cpi	0ffh
