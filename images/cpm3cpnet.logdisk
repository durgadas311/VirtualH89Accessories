BDOS	equ	0005h

BUFLEN	equ	128

; standard BDOS calls
WBOOT	equ	0
CONIN	equ	1
CONOUT	equ	2
PRINT	equ	9
GTLINE	equ	10
GETVER	equ	12
OPEN	equ	15
CLOSE	equ	16
SERFST	equ	17
SERNXT	equ	18
DELETE	equ	19
SREAD	equ	20
SWRITE	equ	21
MAKE	equ	22
RENAME	equ	23
SETDMA	equ	26
SGUSER	equ	32
RREAD	equ	33
RWRITE	equ	34
FSIZE	equ	35
ERRMOD	equ	45
GTTIME	equ	105

; CP/NET (NDOS) calls
NCFGTB	equ	69
NSVCFG	equ	71

; MP/M (XDOS) calls
XPRIO	equ	145
XSDPA	equ	154
; Offsets in SDP
SDPCPN	equ	9	; CP/Net master config table
SDPTMP	equ	196

; Offsets in Server Config Table (as used by CP/Net Clients)
SCTTMP	equ	0	; Temp file drive
SCTSTS	equ	1	; Network (device) status (MP/M SDP entry points here)
SCTNID	equ	2	; Node ID
SCTMAX	equ	3	; Max allowed logins
SCTCUR	equ	4	; Num logged in
SCTVEC	equ	5	; (word) Login vector
SCTRID	equ	7	; 16x1 Table of remote node IDs logged in
SCTLEN	equ	23	; Length of table

; Offsets in Client Config Table
NCTSTS	equ	0	; Network (device) status
NCTNID	equ	1	; Node ID
NCTMAP	equ	2	; 16x2 Map of disk drives
NCTCON	equ	34	; Map for CON:
NCTLST	equ	36	; Map for LST:
NCTIDX	equ	38	; Index into LST: buffer
NCTMSG	equ	39	; CP/NET message buffer for 129-byte payload (LST: buffer)
NCTLEN	equ	173	; Length of table

; Offsets in standard FCB
FCBDRV	equ	0	; Disk drive, 1-16, or 0 for current default
FCBNAM	equ	1	; 8x NAME begins
FCBTYP	equ	9	; 3x TYPE begins
FCBEXT	equ	12	; EXT byte
FCBS1	equ	13	; S1
FCBS2	equ	14	; S2
FCBRC	equ	15	; RC - Record Count of extent
FCBD0	equ	16	; 16x D0-D15, internal use only
FCBREN	equ	FCBD0	; Second name entry for RENAME
FCBCR	equ	32	; CR - current record (within extent)
FCBRR0	equ	33	; 3x RR - random record number
FCBLEN	equ	36	; Length of full FCB

CR	equ	13
LF	equ	10
EOF	equ	26	; End Of Message marker, plus text EOF

; MAIL constants
MSGMXR	equ	13	; max records for MAIL message, excl. subject/header
MSGTYP	equ	0	; Record type - 0AAH, 000H/01AH.
MSGSIZ	equ	1	; Record data len - includes "from" on rec 0
MSGHDR	equ	2	; overhead in records 1-N. Data begins here.
MSGMAX	equ	BUFLEN-MSGHDR	; max length of data in record
; Record 0 - Subject and Envelope information
MSGFRM	equ	2	; From: ID
MSGHD0	equ	3	; record 0 - subject text begins here
MSGTIM	equ	4	; time/date at end of record
MSGMXS	equ	BUFLEN-MSGHD0-MSGTIM	; max length of Subject text
; Record types
MSGSOM	equ	0aah	; Start Of Message marker (first/subject record)
MSGEOM	equ	EOF	; End Of Message marker (last record)
MSGDAT	equ	0	; Middle Record(s) marker.
MSGDEL	equ	0ffh	; Deleted Record(s) marker (until next packfile).

	org	0100h
	jmp	start

	db	'MAIL Version 1.2COPYRIGHT 1982, DIGITAL RESEARCH080282'

start:
	lxi	sp,stack
	mvi	e,0ffh
	mvi	c,ERRMOD
	call	BDOS
	mvi	e,0ffh
	mvi	c,SGUSER
	call	BDOS
	sta	curusr
	mvi	c,GETVER
	call	BDOS
	shld	version
	mov	a,h
	ani	01h
	jnz	mpminit
	mov	a,h
	ani	02h
	jnz	cpninit
; CP/M is an error... for now...
	lxi	d,cpmerr
exit0:
	mvi	c,PRINT
	call	BDOS
exit1:
	lda	curusr
	mov	e,a
	mvi	c,SGUSER
	call	BDOS
	mvi	c,WBOOT
	jmp	BDOS	; no return
;----------------------------------

mpminit:
	mvi	c,XSDPA
	call	BDOS
	mov	e,l
	mov	d,h
	lxi	b,SDPCPN
	dad	b
	mov	a,m
	inx	h
	mov	h,m
	mov	l,a
	push	h	; MP/M Net Srv Cfg Table
	lxi	h,SDPTMP
	dad	d
	mov	a,m
	sta	srvcfg+SCTTMP
	; BUG: by copying table, we use stale data
	; when searching logged-in nodes. Problem
	; increases the longer the user is in MAIL.
	lxi	h,srvcfg+SCTSTS
	pop	d
	lxi	b,22
	ldir
	lda	srvcfg+SCTNID
	sta	myid
	mvi	e,100
	mvi	c,XPRIO
	call	BDOS
	jmp	main

cpninit:
	mvi	c,NCFGTB
	call	BDOS
	shld	netcfg
	inx	h
	mov	a,m
	sta	myid
main:
	lxi	h,msgbuf
	mvi	m,MSGEOM
	inx	h
	mvi	m,0
toploop:
	mvi	a,1
	call	getopt
	ora	a
	jz	bcast
	dcr	a
	jz	send
	dcr	a
	jz	recv
	dcr	a
	jz	exit1
; error?
	jmp	toploop

;;;;;;;; BROADCAST A MESSAGE ;;;;;;;;;;
bcast:
	call	getsubject
	mvi	a,2
	call	getopt
	ora	a
	jz	bcast1
	call	gettext
	jmp	bcast2
bcast1:
	call	getinputfile
bcast2:
	lda	version+1
	ani	02h
	jnz	bccpnet

bcmpm:
	lda	srvcfg+SCTTMP
	sta	mailfcb+FCBDRV
	mvi	b,255	; exclude node - none
	call	bcast5
	jmp	toploop

bccpnet:
bccpn0:
	call	findtmp ; sets up srvcfg
	ora	a
	jz	toploop
	cpi	0ffh
	jz	bccpn0
	sta	mailfcb+FCBDRV
	lda	myid
	mov	b,a	; exclude node - ourself
	call	bcast5
	jmp	bccpn0

bcast5:
	lxi	h,srvcfg+SCTRID
	xchg
	lhld	srvcfg+SCTVEC
	mvi	c,16
bcmpm0:
	mov	a,l
	ani	1
	jz	bcmpm2
	ldax	d	; target node ID
	cmp	b
	jz	bcmpm2
	push	h
	push	d
	push	b
	call	getmailfile	; A still target node ID
	ora	a
	jnz	op1error	; mailfcb
	call	addmsg
	pop	b
	pop	d
	pop	h
bcmpm2:
	rarr	h
	rarr	l
	inx	d
	dcr	c
	jnz	bcmpm0
	ret

;;;;;;;; SEND A MESSAGE ;;;;;;;;;;
send:
	call	getnodeid
	sta	tonode
	call	getsubject
	mvi	a,2
	call	getopt
	ora	a
	jz	send1
	call	gettext
	jmp	send2
send1:
	call	getinputfile
send2:
	lda	version+1
	ani	02h
	jnz	sncpnet
snmpm:
	lda	srvcfg+SCTTMP
	sta	mailfcb+FCBDRV
	lda	tonode
	call	getmailfile
	ora	a
	jnz	op1error	; mailfcb
	call	addmsg
	jmp	toploop

sncpnet:

	; Locate first server or logged-in node matching dest.
	; This code may not be thorough enough.
sncpn0:
	call	findtmp
	ora	a
	jz	sncpn4
	cpi	0ffh
	jz	sncpn0
	sta	mailfcb+FCBDRV
	lda	tonode
	mov	b,a
	lda	srvcfg+SCTNID
	cmp	b
	jnz	sncpn1
	; send to server
	mov	a,b
	call	getmailfile
	cpi	0ffh
	jz	op1error
	call	addmsg
	jmp	sncpn6	; cleanup
	; Search server for logged-in node
sncpn1:
	lxi	h,srvcfg+SCTRID
	xchg
	lhld	srvcfg+SCTVEC
	mvi	c,16
	lda	tonode
	mov	b,a
sncpn2:
	mov	a,l
	ani	1
	jz	sncpn3
	ldax	d	; target node ID
	cmp	b
	jnz	sncpn3
	mov	a,b
	call	getmailfile	; A still target node ID
	ora	a
	jnz	op1error	; mailfcb
	call	addmsg
	jmp	sncpn6	; cleanup
sncpn3:
	rarr	h
	rarr	l
	inx	d
	dcr	c
	jnz	sncpn2
	; need to look for more servers...
	jmp	sncpn0

	; node not logged in (and not a server),
	; find any server with temp drive and deliver there.
sncpn4:
	call	findtmp
	ora	a
	jz	sncpn5
	cpi	0ffh
	jz	sncpn4
	sta	mailfcb+FCBDRV
	lda	tonode
	call	getmailfile
	ora	a
	jnz	op1error
	call	addmsg
sncpn6: ; search until end so findtmp does cleanup
	call	findtmp
	ora	a
	jnz	sncpn6
	jmp	toploop
sncpn5:
	lxi	d,notmps
	jmp	exit0

;;;;;;;; RECEIVE MAIL ;;;;;;;;;;
recv:
	lda	version+1
	ani	02h
	jnz	rcvcpn
	lda	srvcfg+SCTTMP
	sta	mailfcb+FCBDRV	; localtemp set in FCB
	jmp	rcvmpm
rcvcpn:
recv0:
	lda	version+1
	ani	02h
	jz	toploop ; MP/M stops here
	call	findtmp
	ora	a
	jz	recv8	; no cleanup needed, already done in case of zero....
	cpi	0ffh
	jz	recv0
	sta	mailfcb+FCBDRV	; localtemp set in FCB
rcvmpm:
	; localtemp, myid are set elsewhere
	call	viewmail
	ora	a
	jz	recv0
	dcr	a
	jz	toploop
recv8:
	lxi	d,nomore
	mvi	c,PRINT
	call	BDOS
	jmp	toploop

; B = myid, C = localtmp
; localtmp already set in mailfcb+FCBDRV...
; myid is set.
viewmail:
	lxi	h,0
	shld	mrec0
	shld	mrec1
vmail0:
	lda	myid
	call	getmailfile
	ora	a
	jnz	op1error	; mailfcb
	call	rdmail
	ora	a
	jnz	rstop2
	mvi	c,CONIN
	call	BDOS
	mvi	a,3
	call	getopt
	ora	a
	jz	rstop	; 1: Stop Receiving Mail
	dcr	a
	jz	ransw	; 2: Answer Message
	dcr	a
	jz	rdelt	; 3: Delete Message
	dcr	a
	jz	ransdl	; 4: Answer and Delete
	dcr	a
	jnz	vmail0	; 6: Next Message
	lhld	mrec0	; 5: Re-examine current message
	shld	mrec1
	jmp	vmail0

rstop2: ; EOF on mail file
	call	packfile
	mvi	a,2
	ret

rstop:	; 1: Stop Receiving Mail
	call	packfile
	mvi	a,1
	ret

ransw:	; 2: Answer Message
	call	replymsg
	jmp	vmail0

rdelt:	; 3: Delete Message
	call	delmsg
	jmp	vmail0

ransdl: ; 4: Answer and Delete
	call	replymsg
	call	delmsg
	jmp	vmail0

; end viewmail

; Show current menu and get option selection...
; A=currrent menu level (1, 2, 3)
getopt:
	dcr	a
	push	psw
	lxi	d,crlf2
	mvi	c,PRINT
	call	BDOS
	lxi	d,crlf2
	mvi	c,PRINT
	call	BDOS
	pop	psw
	push	psw
	add	a
	mov	e,a
	mvi	d,0
	lxi	h,mnutab
	dad	d
	mov	e,m
	inx	h
	mov	d,m
	mvi	c,PRINT
	call	BDOS
	lxi	d,prompt
	mvi	c,PRINT
	call	BDOS
	mvi	a,2
	sta	cmdline+0
	lxi	d,cmdline
	mvi	c,GTLINE
	call	BDOS
	pop	psw
	mov	e,a
	mvi	d,0
	lxi	h,numopt
	dad	d
	lda	cmdline+1
	cpi	1
	jnz	getop1
	lda	cmdline+2
	sui	'0'
	ora	a
	jz	getop1
	cmp	m
	jnc	getop1
	dcr	a
	ret
getop1:
	mov	a,m
	dcr	a
	ret

; Print filename of FCB in DE.
prfile:
	ldax	d	; drive, 1..16
	dcr	a
	adi	'A'
	push	d
	mov	e,a
	mvi	c,CONOUT
	call	BDOS
	mvi	e,':'
	mvi	c,CONOUT
	call	BDOS
	pop	d
	mvi	c,8
	call	prfil0
	push	d
	mvi	e,'.'
	mvi	c,CONOUT
	call	BDOS
	pop	d
	mvi	c,3
	call	prfil0
	lxi	d,crlf
	mvi	c,PRINT
	jmp	BDOS

prfil0:
	ldax	d
	push	d
	push	b
	mov	e,a
	mvi	c,CONOUT
	call	BDOS
	pop	b
	pop	d
	inx	d
	dcr	c
	jnz	prfil0
	ret

op2error:
	lxi	d,msgfcb
	jmp	opnerror
op1error:
	lxi	d,mailfcb
opnerror:
	lxi	h,operrm
errexit:
	push	d
	xchg
	mvi	c,PRINT
