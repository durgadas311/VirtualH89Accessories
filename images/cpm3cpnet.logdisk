rdmail:
	lxi	d,mailfcb
	mvi	c,FSIZE
	call	BDOS
	cpi	0ffh
	jz	rd1error
	lhld	mrec1
	xchg
	lhld	mailfcb+FCBRR0
	ora	a
	dsbc	d
	jz	rdmai9
	xchg
	shld	mailfcb+FCBRR0
	lxi	h,msgbuf	; recptr
	mvi	c,MSGMXR+1
rdmai0:
	push	h
	push	b
	xchg
	mvi	c,SETDMA
	call	BDOS
	lxi	d,mailfcb
	mvi	c,RREAD
	call	BDOS
	ora	a
	jnz	rd1error
	lhld	mailfcb+FCBRR0
	inx	h
	shld	mailfcb+FCBRR0
	pop	b
	pop	h
	mov	a,m
	cpi	MSGEOM
	jz	rdmai1
	lxi	d,BUFLEN
	dad	d
	dcr	c
	jnz	rdmai0
	jmp	rd1error

rdmai1:
	lhld	mrec1
	shld	mrec0
	lhld	mailfcb+FCBRR0
	shld	mrec1
	lxi	d,mailfcb
	mvi	c,CLOSE
	call	BDOS
	cpi	0ffh
	jz	rd1error
rdmai2:
	; TODO handle status bit(s)...
	lda	msgbuf
	cpi	MSGSOM
	jnz	rd1error
	lxi	d,crlf2
	mvi	c,PRINT
	call	BDOS
	lxi	d,l1e2ah
	mvi	c,PRINT
	call	BDOS
	lda	msgbuf+MSGFRM
	call	hexout
	; TODO handle date/time
	lxi	d,l1e3eh
	mvi	c,PRINT
	call	BDOS
	lxi	h,msgbuf+BUFLEN-4
	call	prdate
	lxi	d,l1e3dh
	mvi	c,PRINT
	call	BDOS
	lxi	h,msgbuf+MSGHD0
	lda	msgbuf+MSGSIZ
	dcr	a	; don't count From ID
	mov	c,a
	call	strout
	lxi	d,crlf2
	mvi	c,PRINT
	call	BDOS
	lxi	h,msgbuf+BUFLEN ; recptr
	mvi	c,MSGMXR
rdmai4:
	push	h
	push	b
	mov	a,m	; MSGTYP
	inx	h
	cpi	MSGEOM
	jnz	rdmai5
	mov	c,m	; MSGSIZ
	jmp	rdmai6
rdmai5:
	mvi	c,MSGMAX
rdmai6:
	inx	h
	call	strout
	pop	b
	pop	h
	mov	a,m	; MSGTYP
	cpi	MSGEOM
	mvi	a,0
	rz
	lxi	d,BUFLEN
	dad	d	; HL (recptr) += BUFLEN
	dcr	c
	jnz	rdmai4
	jmp	rd1error
rdmai9:
	lxi	d,mailfcb
	mvi	c,CLOSE
	call	BDOS
	cpi	0ffh
