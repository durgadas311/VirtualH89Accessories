	lxi	h,wrerrm
	jmp	errexit
	

; Return:
;  0:  No (more) servers,
;  0ffh:   Error, or temp drive not mapped
;  1-16:   Temp drive is local drive A-P
; Local vars:
tmpdrv: db	0
tmploc: db	0
curdrv: db	0
tmpff:	db	0

findtmp:
	mvi	a,0ffh
	sta	tmploc
	sta	tmpdrv
	mov	b,a	; cursrv
	mvi	c,020h	; tmpflg
	xra	a
	sta	curdrv
	lhld	netcfg
	inx	h	;
	inx	h	; +NCTMAP
findt0:
	mov	a,m
	ani	0c0h
	ora	c
	sta	tmpff
	cpi	0a0h
	jnz	findt1
	mvi	c,0
	inx	h
	mov	b,m	; cursrv
	dcx	h
	push	h
	push	b
	mov	e,b
	mvi	c,NSVCFG
	call	BDOS
	mov	a,l
	cpi	0ffh
	jnz	findt5
	mov	a,h
	cpi	00ch
	jnz	findt5
	mvi	a,0ffh
	pop	b
	pop	h
	ret
findt5: ; HL=valid server config tbl
	lxi	d,srvcfg
	lxi	b,SCTLEN
	ldir
	lda	srvcfg+SCTTMP	; server tmp drive
	sta	tmpdrv
	pop	b
	pop	h
findt1:
	lda	tmpff
	ani	0c0h
	cpi	080h
	jnz	findt2
	inx	h
	mov	a,m
	dcx	h
	cmp	b
	jnz	findt2
	mvi	a,040h
	ora	m
	mov	m,a
	ani	0fh
	mov	e,a
	lda	tmpdrv
	cmp	e
	jnz	findt2
	lda	curdrv
	inr	a
	sta	tmploc
findt2:
	inx	h
	inx	h
	lda	curdrv
	inr	a
	sta	curdrv
	cpi	16
	jc	findt0
	mov	a,b
	cpi	0ffh
	jnz	findt3
	; cleanup - end of servers
findinit:
	xra	a
	sta	tmploc
	sta	curdrv
	lhld	netcfg
	inx	h	;
	inx	h	; +NCTMAP
	mvi	c,16
findt4: ; cleanup - reset temp flags
	mvi	a,08fh
	ana	m
	mov	m,a
	inx	h
	inx	h
	dcr	c
	jnz	findt4
	xra	a
	ret	; no (more) servers
findt3:
	lda	tmploc
	ret

; A = target node ID, drive set in mailfcb
getmailfile:
	; don't overwrite FCBDRV, already setup...
	lxi	h,mailtex+FCBNAM
	lxi	d,mailfcb+FCBNAM
	lxi	b,FCBLEN-FCBNAM
	ldir
	push	psw
	rrc
	rrc
	rrc
	rrc
	call	tohex
	sta	mailfcb+FCBNAM
	pop	psw
	call	tohex
	sta	mailfcb+FCBNAM+1
	lxi	d,dskbuf
	mvi	c,SETDMA
	call	BDOS
	lxi	d,mailfcb
	mvi	c,SERFST
	call	BDOS
	mvi	c,OPEN
	cpi	0ffh
	jnz	getmf1
	mvi	c,MAKE
getmf1:
	lxi	d,mailfcb
	call	BDOS
	cpi	0ffh
	rnz
	lxi	h,48
getmf2:
	push	h
	lxi	d,mailfcb
	mvi	c,OPEN
	call	BDOS
	pop	h
	cpi	0ffh
	rnz
	dcx	h
	mov	a,l
	ani	03h
	cpi	1
	jnz	getmf3
	push	h
	lxi	d,waitm
	mvi	c,PRINT
	call	BDOS
	pop	h
getmf3:
	mov	a,l
	ora	h
	jnz	getmf2
	mvi	a,0ffh
	ret

; Used by getinputfile(internally) and gettext/nxtbyte(shared)
bytcnt: db	0
; Purely local to getinputfile
dptr:	dw	0

getinputfile:
	lda	curusr
	mov	e,a
	mvi	c,SGUSER
	call	BDOS
	lxi	d,l1dd8h
	mvi	c,PRINT
	call	BDOS
	mvi	a,15
	sta	cmdline+0
	lxi	d,cmdline
	mvi	c,GTLINE
	call	BDOS
	lxi	h,cmdline+2
	lda	cmdline+1
	mov	e,a
	mvi	d,0
	inx	h
	dad	d
	mvi	m,0	; NUL terminate
	lxi	h,blankfile
	lxi	d,msgfcb
	lxi	b,FCBLEN
	ldir
	mvi	c,8
	lxi	d,msgfcb+FCBNAM
	lxi	h,cmdline+2
	lda	cmdline+3
	cpi	':'
	jnz	getif1
	mov	a,m
	inx	h
	inx	h
	sui	'A'
	inr	a
	sta	msgfcb+FCBDRV
getif1:
	mov	a,m
	ora	a
	jz	getif2
	inx	h
	cpi	'.'
	jz	getif2
	stax	d
	inx	d
	dcr	c
	jnz	getif1
getif2:
	mvi	c,3
getif3:
	mov	a,m
	ora	a
	jz	getif4
	inx	h
	stax	d
	inx	d
	dcr	c
	jnz	getif3
getif4:
	lxi	d,msgfcb
	mvi	c,OPEN
	call	BDOS
	cpi	0ffh
	jz	op2error
	lxi	d,dskbuf
	mvi	c,SETDMA
	call	BDOS

	mvi	c,MSGMAX	; C=offset
	mvi	b,MSGMXR	; max reccnt
	lxi	d,msgbuf+BUFLEN ; DE=recptr
	lxi	h,2
	dad	d	; HL=ptr
	xra	a
	sta	bytcnt
getif5:
	push	d
	push	b
	push	h
	lxi	d,msgfcb
	mvi	c,SREAD
	call	BDOS
	ora	a
	jnz	getif6	; must pop!
	pop	h
	pop	b
	push	b
	push	h
	lxi	d,dskbuf
	xchg
	mvi	b,0
	ldir		; HL=(dptr += offset)
	shld	dptr
	lxi	h,bytcnt
	inr	m
	inr	m	; bytcnt += 2
	pop	h
	pop	b
	pop	d
	mvi	a,MSGDAT
	stax	d	; rec type 0 - more follows...
	dcr	b
	jz	getif7	; overflow
	lxi	h,BUFLEN
	dad	d
	xchg		; recptr += BUFLEN;
	lxi	h,2
	dad	d	; ptr = recptr + 2;
	dcr	c
	dcr	c	; offset -= 2;
	push	d
	push	b
	xchg		; DE=ptr
	lhld	dptr
	lda	bytcnt
	mov	c,a
	mvi	b,0
	ldir		; DE=(ptr+=bytcnt)
	xchg		; HL=ptr
	pop	b
	pop	d
	jmp	getif5
getif7:
	mvi	a,MSGEOM
	stax	d
	inx	d
	mvi	a,MSGMAX
	stax	d
getif9:
	lxi	d,msgfcb
	mvi	c,CLOSE
	call	BDOS
	mvi	e,0
	mvi	c,SGUSER
	jmp	BDOS
getif6: ; EOF or error...
	cpi	1
	jnz	rd2error	; no need to pop...
	; look for ASCII EOF...
	pop	h	; ptr
	pop	b	; offset
	pop	d	; recptr
	xchg
	dcr	h	; HL -= 256
	push	h
	lxi	d,2
	dad	d
	lxi	b,MSGMAX
	mvi	a,EOF
	ccir
	pop	h
	jnz	getif8
	mov	m,a
	inx	h
	mvi	a,MSGMAX
	sub	c
	mov	m,a
	jmp	getif9
getif8:
	lxi	d,BUFLEN
	dad	d
	push	h
	inx	h
	mov	c,m	; length
	mvi	b,0
	inx	h
	mvi	a,EOF
	ccir
	pop	h
	mov	m,a
	inx	h
	mvi	a,MSGMAX
	sub	c	; C could be 0
	mov	m,a
	jmp	getif9

; Used by gettext/nxtbyte to share context
reccnt: db	0
recptr: dw	0
msgptr: dw	0

; A = bytcnt, returns new bytcnt
nxtbyte:
	lda	bytcnt
	inr	a
	sta	bytcnt	; not needed if end of rec...
	lhld	msgptr
	inx	h
	shld	msgptr	; not needed if end of rec...
	cpi	MSGMAX
	rc	; within same record, keep going.
	lhld	recptr
	mvi	m,MSGDAT
	lda	reccnt	; include header/subject record
	inr	a
	cpi	MSGMXR+1
	jnc	nxtby1	; overflow
	sta	reccnt	; point to next record
	lxi	d,BUFLEN
	dad	d
	shld	recptr
	inx	h
	inx	h
	shld	msgptr
	xra	a
	sta	bytcnt
	ret
nxtby1:
	mvi	m,MSGEOM
	inx	h
	mvi	m,MSGMAX
	mvi	a,0ffh
	ret

gettext:
	lxi	d,entmsg
	mvi	c,PRINT
	call	BDOS
	lxi	d,crlf
	mvi	c,PRINT
	call	BDOS
	mvi	a,78
	sta	cmdline+0
	mvi	a,1
	sta	reccnt
	lxi	h,msgbuf+BUFLEN
	shld	recptr
	inx	h
	inx	h
	shld	msgptr
	xra	a
	sta	bytcnt
gettx0:
	lxi	d,cmdline
	mvi	c,GTLINE
	call	BDOS
	lxi	d,crlf
	mvi	c,PRINT
	call	BDOS
	lxi	h,cmdline+1
	mov	e,m
	mvi	d,0
	inx	h
	dad	d
	mvi	m,0	; NUL terminate
	lxi	h,cmdline+2
gettx1:
	mov	a,m
	ora	a
	jz	gettx2
	cpi	EOF
	jz	gettx9
	inx	h
	push	h
	lhld	msgptr
	mov	m,a
	call	nxtbyte
	cpi	0ffh
	pop	h
	jnz	gettx1
	ret	; overflow
gettx2:
	push	h
	lhld	msgptr
	mvi	m,CR
	call	nxtbyte
	cpi	0ffh
	pop	h
	rz
	push	h
	lhld	msgptr
	mvi	m,LF
	call	nxtbyte
	cpi	0ffh
	pop	h
	rz
	jmp	gettx0
gettx9:
	lhld	recptr
	mvi	m,MSGEOM
	inx	h
	lda	bytcnt
	mov	m,a
	ret

getsubject:
	mvi	a,80
	sta	cmdline+0
	lxi	d,l1debh
	mvi	c,PRINT
	call	BDOS
	lxi	d,cmdline
	mvi	c,GTLINE
	call	BDOS
	lxi	h,msgbuf
	mvi	m,MSGSOM
	inx	h
	lda	cmdline+1
	mov	c,a
	inr	a
	mov	m,a
	inx	h
	lda	myid
	mov	m,a
	inx	h
	xchg
	lxi	h,cmdline+2
	mvi	b,0
	ldir
	lxi	h,msgbuf+BUFLEN-MSGTIM
	call	stdate
	ret

addmsg:
	lxi	d,mailfcb
	mvi	c,FSIZE
	call	BDOS
	cpi	0ffh
	jz	wr1error
	lxi	h,msgbuf
	mvi	c,MSGMXR+1
addmg0:
	push	h
	push	b
	xchg
	mvi	c,SETDMA
	call	BDOS
	lxi	d,mailfcb
	mvi	c,RWRITE
	call	BDOS
	ora	a
	jnz	wr1error
	lhld	mailfcb+FCBRR0
	inx	h
	shld	mailfcb+FCBRR0
	pop	b
	pop	h
	mov	a,m
	cpi	MSGEOM
	jz	addmg1
	lxi	d,BUFLEN
	dad	d
	dcr	c
	jnz	addmg0
addmg1:
	lxi	d,mailfcb
	mvi	c,CLOSE
	call	BDOS
	cpi	0ffh
	jz	wr1error
	ret

; assume < 100
decout:
	mvi	b,'0'
decot0:
	sui	10
	jc	decot1
	inr	b
	jmp	decot0
decot1:
	adi	10
	adi	'0'
	push	psw
	mov	a,b
	call	prout
	pop	psw
	jmp	prout

hexout:
	push	psw
	rrc
	rrc
	rrc
	rrc
	call	hexdig
	pop	psw
hexdig:
	call	tohex
prout:
	mov	e,a
	mvi	c,CONOUT
	jmp	BDOS

tohex:
	ani	0fh
	adi	90h
	daa
	aci	40h
	daa
	ret

; Keeps number in HL - caller must preserve/init
; Returns CY for invalid
hexnum:
	sui	'0'
	rc
	cpi	9+1
	jnc	hexnm1
hexnm2:
	dad	h
	dad	h
	dad	h
	dad	h
	ora	l
	mov	l,a
	ret
hexnm1:
	sui	'A'-'9'
	rc
	cpi	5+1
	cmc
	rc
	adi	10
	jmp	hexnm2


; prompt for node ID and parse input into hex number
getnodeid:
	mvi	a,3
	sta	cmdline+0
getni0:
	lxi	d,l1dffh
	mvi	c,PRINT
	call	BDOS
	lxi	d,cmdline
	mvi	c,GTLINE
	call	BDOS
	lda	cmdline+1
	ora	a
	jz	getni2
	cpi	3
	jnc	getni2
	lxi	h,0
	lxi	d,cmdline+2
	lda	cmdline+1
	mov	c,a
getni1:
	ldax	d
	inx	d
	call	hexnum
	jc	getni2
	dcr	c
	jnz	getni1
	mov	a,l
	ret
getni2:
	lxi	d,l1e12h
	mvi	c,PRINT
	call	BDOS
	jmp	getni0

; HL=string, C=length
strout:
	mov	a,c
	ora	a
	rz
strou0:
	mov	e,m
	push	h
	push	b
	mvi	c,CONOUT
	call	BDOS
	pop	b
	pop	h
	inx	h
	dcr	c
	jnz	strou0
