delmsg:
	lda	myid
	call	getmailfile
	cpi	0ffh
	jz	op1error
	lxi	h,msgbuf
	mvi	m,MSGDEL
	lxi	d,msgbuf+1
	lxi	b,BUFLEN-1
	ldir	; memset entire record - no shadow data
	lxi	d,msgbuf
	mvi	c,SETDMA
	call	BDOS
	lhld	mrec0
	shld	mailfcb+FCBRR0
delmg0:
	lxi	d,mailfcb
	mvi	c,RWRITE
	call	BDOS
	ora	a
	jnz	wr1error
	lhld	mailfcb+FCBRR0
	inx	h
	shld	mailfcb+FCBRR0
	xchg
	lhld	mrec1
	xchg
	ora	a
	dsbc	d
	jc	delmg0
	lxi	d,mailfcb
	mvi	c,CLOSE
