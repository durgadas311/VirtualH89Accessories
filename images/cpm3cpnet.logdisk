replymsg:
	lxi	h,msgbuf+MSGMXS-restrl
	lxi	d,msgbuf+MSGMXS
	lxi	b,MSGMXS-restrl
	lddr
	lxi	h,restr
	lxi	d,msgbuf+MSGHD0
	lxi	b,restrl
	ldir
	lxi	h,msgbuf+BUFLEN-MSGTIM
	call	stdate
	lxi	h,msgbuf+MSGSIZ
	mvi	a,restrl
	add	m
	cpi	MSGMXS+1	; length includes "from" byte
	jc	rpymg1
	mvi	a,MSGMXS+1
rpymg1:
	mov	m,a
