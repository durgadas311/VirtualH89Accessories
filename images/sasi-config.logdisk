DPH6:	DW	0,0,0,0,DIRBUF,DPB6,CSV0,ALV6
DPH7:	DW	0,0,0,0,DIRBUF,DPB7,CSV0,ALV7
DPH8:	DW	0,0,0,0,DIRBUF,DPB8,CSV0,ALV8

CNUM:	DB	0		; CONTROLLER NUMBER 0

